{
    "name": "terciuss/laravel-swagger",
    "description": "Laravel Swagger generator for the Model",
    "keywords": ["laravel", "swagger", "swagger-php", "model", "eloquent"],
    "type": "extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Upton",
            "email": "kevin@upton.tech"
        }
    ],
    "require": {
        "php": "^8.0",
        "zircote/swagger-php": "^2.0",
        "doctrine/dbal": "^3"
    },
    "autoload": {
        "psr-4": {
            "Kevupton\\LaravelSwagger\\": "src/Kevupton/LaravelSwagger/"
        },
        "files": [
            "src/Kevupton/LaravelSwagger/functions.php"
        ]
    },
    "minimum-stability": "stable",
    "require-dev": {
        "laravel/laravel": "^9.0",
        "laravel/lumen": "^9.0"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
